{
  "name": "timedoor/mail-logger",
  "description": "Outcoming email logger for Laravel",
  "keywords": [
    "laravel",
    "mail",
    "email",
    "log",
    "logger"
  ],
  "type": "library",
  "require": {
    "php": ">=7.1.3",
    "laravel/framework": "^7.0|^8.0|^9.0"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Timedoor\\MailLogger\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Timedoor\\MailLogger\\MailLoggerServiceProvider"
      ]
    }
  },
  "authors": [
    {
      "name": "Suhadak Akbar",
      "email": "ggsuha@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true
}
